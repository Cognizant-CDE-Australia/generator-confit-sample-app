{
  "name": "ts-ng-2-stylus",
  "version": "0.0.0",
  "description": "A description of my awesome package",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://example.com/entity/repo"
  },
  "scripts": {
    "build": "npm-run-all clean:prod build:prod",
    "build:dev": "webpack-dev-server --progress --config config/webpack/dev.webpack.config.js --hot",
    "build:prod": "webpack -p --progress --config config/webpack/prod.webpack.config.js",
    "build:serve": "npm-run-all build serve:prod",
    "clean:dev": "rimraf dev/",
    "clean:prod": "rimraf dist/",
    "dev": "npm-run-all clean:dev verify --parallel verify:watch build:dev serve:dev",
    "release": "npm-run-all build verify test:unit:once",
    "serve:dev": "echo Starting DEV server",
    "serve:prod": "npm run serve:prod:https",
    "serve:prod:http": "serve -C -p 3000 dist/",
    "serve:prod:https": "serve-https -p 3000 -d dist/ --servername localhost",
    "start": "npm run dev",
    "test": "npm run test:unit",
    "test:browser": "protractor config/testBrowser/protractor.conf.js",
    "test:unit": "karma start ./config/testUnit/karma.conf.js",
    "test:unit:debug": "karma start ./config/testUnit/karma.conf.js --debug",
    "test:unit:once": "karma start --singleRun=true ./config/testUnit/karma.conf.js",
    "typings:install": "typings install",
    "verify": "npm-run-all verify:css verify:js --silent",
    "verify:css": "stylint -c config/verify/.stylintrc src/",
    "verify:css:watch": "chokidar 'src/**/*.styl' -c 'npm run verify:css' --initial --silent",
    "verify:js": "tslint -c config/verify/tslint.json \"src/**/*.ts\" \"unitTest/**/*.ts\" \"config/**/*.ts\"",
    "verify:js:watch": "chokidar 'src/**/*.ts' 'unitTest/**/*.ts' 'config/**/*.ts' -c 'npm run verify:js' --initial --silent",
    "verify:watch": "npm-run-all --parallel verify:css:watch verify:js:watch --silent"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/release/commitMessageConfig.js"
    },
    "ghooks": {
      "commit-msg": "./node_modules/cz-customizable-ghooks/index.js $2",
      "pre-push": "npm-run-all verify test:unit:once --silent"
    }
  },
  "dependencies": {
    "angular2": "2.0.0-beta.17",
    "core-js": "2.3.0",
    "rxjs": "5.0.0-beta.7",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "awesome-typescript-loader": "0.17.0",
    "chokidar-cli": "1.2.0",
    "css-loader": "0.23.1",
    "cz-customizable": "4.0.0",
    "cz-customizable-ghooks": "1.1.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "ghooks": "1.2.1",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.7.1",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "2.4.1",
    "jasmine-reporters": "2.1.1",
    "jasmine-spec-reporter": "2.4.0",
    "karma": "0.13.22",
    "karma-chrome-launcher": "0.2.3",
    "karma-coverage": "0.5.5",
    "karma-jasmine": "0.3.8",
    "karma-junit-reporter": "0.4.2",
    "karma-ng-html2js-preprocessor": "0.2.0",
    "karma-phantomjs-launcher": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.23",
    "karma-threshold-reporter": "0.1.15",
    "karma-webpack": "1.7.0",
    "lodash": "3.10.1",
    "npm-run-all": "2.1.1",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "2.1.7",
    "postcss-loader": "0.8.2",
    "protractor": "3.2.1",
    "rimraf": "2.5.2",
    "serve": "1.4.0",
    "serve-https": "1.5.3",
    "style-loader": "0.13.0",
    "stylint": "1.5.6",
    "stylus": "0.54.5",
    "stylus-loader": "2.1.1",
    "ts-helpers": "1.1.1",
    "tslint": "3.8.1",
    "typescript": "1.8.9",
    "webpack": "1.12.11",
    "webpack-dev-server": "1.14.1"
  },
  "private": true
}
